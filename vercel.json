{
  "version": 2,
  "name": "Podcast",
  "builds": [
      {
          "use": "@now/static-build",
          "src": "package.json"
      }
  ],
  "routes": [
      {
          "src": "(.*).js",
          "dest": "/$1.js"
      },
      {
          "src": "(.*).scss",
          "dest": "/$1.scss"
      },
      {
          "src": "(.*).css",
          "dest": "/$1.css"
      },
      {
          "src": "(.*).jpg",
          "dest": "/$1.jpg"
      },
      {
          "src": "(.*).jpeg",
          "dest": "/$1.jpeg"
      },
      {
          "src": "(.*).svg",
          "dest": "/$1.svg"
      },
      {
          "src": "(.*).png",
          "dest": "/$1.png"
      },
      {
          "src": "(.*).json",
          "dest": "/$1.json"
      },
      {
          "src": "/.*",
          "dest": "index.html"
      }
  ]
}